  {
    "name": "Personal Goals coach",
    "version": "0.1",
    "description": "Create a nagging assistant who checks up on you",
    "permissions": ["activeTab", "declarativeContent", "storage", "notifications"],
    "background": {
        "scripts": ["moment.js", "background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "assistant.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "manifest_version": 2
  }